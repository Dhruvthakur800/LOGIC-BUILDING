// Program: Find LCM of Two Numbers using Recursion
#include <stdio.h>
int lcm(int a, int b, int multiple) {
    if(multiple % a == 0 && multiple % b == 0) return multiple;
    return lcm(a,b,multiple+1);
}
int main() {
    int a,b;
    printf("Enter two numbers: ");
    scanf("%d %d",&a,&b);
    printf("LCM = %d\n", lcm(a,b,(a>b)?a:b));
    return 0;
}
