// Program: Copy One Array to Another
#include <stdio.h>
int main() {
    int n;
    printf("Enter number of elements: ");
    scanf("%d",&n);
    int arr[n], copy[n];
    for(int i=0;i<n;i++) scanf("%d",&arr[i]);
    for(int i=0;i<n;i++) copy[i] = arr[i];
    printf("Copied Array: ");
    for(int i=0;i<n;i++) printf("%d ",copy[i]);
    return 0;
}
